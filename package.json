{
	"name": "vitamin",
	"license": "MIT",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"build": "tsc && vite build",
		"commit": "cz",
		"dev": "vite --open",
		"prepare": "husky install",
		"serve": "vite preview",
		"serve:test": "start-server-and-test 'yarn serve' http://localhost:5000",
		"test": "jest --watch",
		"test:ci": "jest --ci",
		"test:e2e": "yarn serve:test 'cypress open'",
		"test:e2e:ci": "yarn serve:test 'cypress run'",
		"format": "prettier -uw --ignore-path .gitignore .",
		"run-tsc": "tsc",
		"run-eslint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.tsx src",
		"run-stylelint": "stylelint --cache --fix --ignore-path .gitignore '**/*.css'",
		"lint": "npm-run-all --parallel run-tsc run-stylelint run-eslint",
		"validate": "yarn lint && yarn build --mode test && yarn test:e2e:ci"
	},
	"dependencies": {
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-query": "3.31.0",
		"react-router-dom": "5.3.0"
	},
	"devDependencies": {
		"@babel/core": "7.15.8",
		"@babel/preset-env": "7.15.8",
		"@babel/preset-react": "7.14.5",
		"@babel/preset-typescript": "7.15.0",
		"@cypress/code-coverage": "3.9.11",
		"@nabla/vite-plugin-eslint": "1.3.2",
		"@tailwindcss/forms": "0.3.4",
		"@testing-library/jest-dom": "5.14.1",
		"@testing-library/react": "12.1.2",
		"@testing-library/user-event": "13.3.0",
		"@types/jest": "27.0.2",
		"@types/react": "17.0.33",
		"@types/react-dom": "17.0.10",
		"@types/react-router-dom": "5.3.2",
		"@typescript-eslint/eslint-plugin": "4.33.0",
		"@typescript-eslint/parser": "4.33.0",
		"@vitejs/plugin-react": "1.0.7",
		"autoprefixer": "10.4.0",
		"babel-jest": "27.2.5",
		"cypress": "8.7.0",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "7.32.0",
		"eslint-config-airbnb": "18.2.1",
		"eslint-config-airbnb-base": "14.2.1",
		"eslint-config-airbnb-typescript": "14.0.1",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-import": "2.25.2",
		"eslint-plugin-jest": "25.0.5",
		"eslint-plugin-jest-dom": "3.9.2",
		"eslint-plugin-jsx-a11y": "6.4.1",
		"eslint-plugin-react": "7.26.1",
		"eslint-plugin-react-hooks": "4.2.0",
		"eslint-plugin-react-prefer-function-component": "0.0.7",
		"eslint-plugin-testing-library": "4.12.4",
		"eslint-plugin-unicorn": "37.0.1",
		"husky": "7.0.4",
		"jest": "27.2.5",
		"jest-watch-typeahead": "1.0.0",
		"lint-staged": "11.2.6",
		"msw": "0.35.0",
		"npm-run-all": "4.1.5",
		"postcss": "8.3.11",
		"prettier": "2.4.1",
		"rollup": "2.59.0",
		"rollup-plugin-istanbul": "3.0.0",
		"start-server-and-test": "1.14.0",
		"stylelint": "14.0.1",
		"stylelint-config-prettier": "9.0.3",
		"stylelint-config-standard": "23.0.0",
		"tailwindcss": "2.2.19",
		"typescript": "4.4.4",
		"vite": "2.6.13",
		"vite-plugin-pwa": "0.11.3",
		"vite-tsconfig-paths": "3.3.17",
		"whatwg-fetch": "3.6.2",
		"workbox-build": "6.3.0",
		"workbox-window": "6.3.0"
	},
	"browserslist": {
		"production": "Edge >= 18, Firefox >= 60, Chrome >= 61, Safari >= 11, Opera >= 48",
		"development": [
			"last 1 chrome version",
			"last 1 firefox version"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"lint-staged": {
		"*": "prettier -uw",
		"*.css": "stylelint --cache --fix",
		"*.{js,ts,tsx}": [
			"eslint --cache --fix",
			"jest --findRelatedTests"
		]
	}
}
