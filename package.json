{
  "name": "vitamin",
  "license": "MIT",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "commit": "cz",
    "dev": "vite --open",
    "prepare": "husky install",
    "serve": "vite preview",
    "serve:test": "start-server-and-test 'yarn serve' http://localhost:5000",
    "test": "yarn serve:test 'cypress open'",
    "test:headless": "yarn serve:test 'cypress run'"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.23.2",
    "react-router-dom": "5.3.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.10",
    "@tailwindcss/forms": "0.3.3",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.8",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "autoprefixer": "10.3.4",
    "cypress": "8.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "14.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "35.0.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "postcss": "8.3.6",
    "prettier": "2.4.0",
    "rollup-plugin-istanbul": "3.0.0",
    "start-server-and-test": "1.14.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "22.0.0",
    "tailwindcss": "2.2.15",
    "typescript": "4.4.3",
    "vite": "2.5.6",
    "vite-plugin-pwa": "0.8.1",
    "vite-tsconfig-paths": "3.3.14"
  },
  "browserslist": "Edge >= 18, Firefox >= 60, Chrome >= 61, Safari >= 11, Opera >= 48",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.css": "stylelint --fix",
    "*.{html,js,ts,tsx,css,md,json}": "prettier --write",
    "*.{js,ts,tsx}": "eslint --cache --fix"
  }
}
